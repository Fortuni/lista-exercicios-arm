#include "main.h"

int main(void)
{

		RCC->AHB1ENR=0X087;  //liga o clock
		GPIOA->MODER|=0X28555500; //PA 5 COMO SAIDA
		RCC->APB2ENR|=0X20000; //HABILITA O CLOCK DO TIMER 11
		GPIOC->MODER|=0X1;
			TIM10->CR1|=0X05;
			TIM10->PSC=3199;
			TIM10->ARR=999;


  while (1)
  {

	 if((TIM10->SR &0x01)==0x01){
		 GPIOC->ODR ^=0X1;//INVERTE ESTADO PC0
		 TIM10 ->SR &=~(0X01);//LIMPANDO FLAG DO TIMER10
	 }
  }
}
